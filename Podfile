  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'OpenCV'

  # Pods for iOSOpenCVTest

  target 'iOSOpenCVTestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iOSOpenCVTestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
